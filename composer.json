{
    "name": "aaronfrancis/reservable",
    "description": "Eloquent model reservation/locking through Laravel's cache lock system",
    "keywords": ["laravel", "eloquent", "lock", "reservation", "cache"],
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Francis",
            "email": "aarondfrancis@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/cache": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "nesbot/carbon": "^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^9.0|^10.8",
        "pestphp/pest": "^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^3.0|^4.0"
    },
    "scripts": {
        "test": "pest",
        "lint": "pint"
    },
    "autoload": {
        "psr-4": {
            "AaronFrancis\\Reservable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AaronFrancis\\Reservable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AaronFrancis\\Reservable\\ReservableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
